<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="UserSevice" class="D20230915.Demo.sevice.impl.UserSeviceSimple" >
        <constructor-arg ref="UserDao"/>
    </bean>
    <bean id="UserDao" class="D20230915.Demo.dao.impl.Simple"></bean>
    <bean id="InjectDemoUserDao" class="D20230915.Demo.dao.impl.InjectDemoUserDao">
        <constructor-arg type="int" value="8080"/>
        <constructor-arg type="int[]" value="1,2,3"/>
        <constructor-arg>
            <list>
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </list>
        </constructor-arg>
        <constructor-arg>
            <set>
                <value>haha</value>
                <value>heihei</value>
                <value>huhu</value>
            </set>
        </constructor-arg>
        <constructor-arg>
            <map>
               <entry key="name" value="mingzi"/>
               <entry key="pwd" value="mima"/>
               <entry key="account" value="zhanghao"/>
            </map>
        </constructor-arg>
        <constructor-arg>
            <props>
                <prop key="administrator">administrator@example.org</prop>
                <prop key="support">support@example.org</prop>
                <prop key="development">development@example.org</prop>
            </props>
        </constructor-arg>
    </bean>
</beans>